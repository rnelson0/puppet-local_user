{
  "name": "rnelson0-local_user",
  "version": "1.0.9",
  "author": "Rob Nelson",
  "summary": "Manage local users, setting an initial password and letting the user manage it as needed afterward.",
  "license": "Apache-2.0",
  "source": "https://github.com/rnelson0/puppet-local_user",
  "project_page": "https://github.com/rnelson0/puppet-local_user",
  "issues_url": "https://github.com/rnelson0/puppet-local_user/issues",
  "dependencies": [
    {
