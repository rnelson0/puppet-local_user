      "version_requirement": ">= 3.7.0 < 2015.4.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">=3.5.0 <5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
